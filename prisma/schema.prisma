datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}


model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  firstName String
  lastName String
  password String
}
